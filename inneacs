#!/bin/bash

do_one()
{
rm -f $1.conf
cat ${HOME}/gaeilge/aimsigh/aimsigh/NNN.conf | sed "s/NNN/$1/" > $1.conf
swish-e -c $1.conf -f $1.tmp -S prog -v 1
mv -f $1.tmp $1.index
mv -f $1.tmp.prop $1.index.prop
}

cd /home/kps/seal/inneacs
do_one TEIDIL
do_one NNN
do_one YNN
do_one YYN
do_one NNY
do_one YNY
do_one YYY
